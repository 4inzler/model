# =============== SEL Discord Bot (.env) ===============
# Fill in DISCORD_TOKEN and keep the rest unless you know you want different behavior.

# Discord
DISCORD_TOKEN=MTQzMDcxNjM3OTM1NDM3MDA2OA.G8XDkA.b6jL1gFnIaA5GCIWUvwP5kKZNhOeCRRCmP0N4s
SEL_DISABLE_PRIVILEGED_INTENTS=0

# Models (container paths; you mount /home/rinz/models -> /models)
SEL_GENERAL_MODEL=/models/fusechat-7b
SEL_CODER_MODEL=/models/fusecoder-7b

# Human feel and behavior
SEL_MODE=auto
SEL_STREAM=1
SEL_MAX_NEW_TOKENS=256
SEL_SHOW_MOOD=0
SEL_RESPOND_TO_ALL=1
SEL_AUTO_REPLY_CHANCE=0.55
SEL_AUTO_REPLY_COOLDOWN=12
SEL_FOLLOWUP_WINDOW=120
SEL_DEEP_THINKING=1
SEL_REFLECTION_STEPS=3
SEL_REFLECTION_TOKENS=200
SEL_PROMPT_HISTORY_TURNS=10

# Stability and performance
SEL_SINGLE_BACKEND=0
SEL_DROP_OLD_SECONDS=30
SEL_QUANT=4bit

# Logging
SEL_LOG_LEVEL=DEBUG

# Check-ins (human, gentle nudges) — off by default
SEL_CHECKINS=0
#SEL_CHECKIN_MINUTES=45
#SEL_CHECKIN_GAP_MINUTES=180
#SEL_DM_CHECKIN_MINUTES=60

# TTS (Piper) — keep off unless you set a voice model
PIPER_MODEL=/models/piper/en_US-amy-medium.onnx
SEL_TTS_DEFAULT=0
SEL_DISCORD_TOKEN=MTQzMDcxNjM3OTM1NDM3MDA2OA.G8XDkA.b6jL1gFnIaA5GCIWUvwP5kKZNhOeCRRCmP0N4s
